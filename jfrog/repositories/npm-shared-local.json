{
  "key": "npm-shared-local",
  "rclass": "local",
  "packageType": "npm",
  "description": "Shared local repository for both web-dev and ios teams",
  "notes": "Organized by scopes: @web-dev/*, @ios/*, @shared/*",
  "includesPattern": "**/*",
  "excludesPattern": "",
  "repoLayoutRef": "npm-default",
  "debianTrivialLayout": false,
  "checksumPolicyType": "client-checksums",
  "handleReleases": true,
  "handleSnapshots": true,
  "maxUniqueSnapshots": 0,
  "maxUniqueTags": 0,
  "snapshotVersionBehavior": "unique",
  "suppressPomConsistencyChecks": false,
  "blackedOut": false,
  "xrayIndex": true,
  "propertySets": [],
  "archiveBrowsingEnabled": false,
  "calculateYumMetadata": false,
  "yumRootDepth": 0,
  "dockerApiVersion": "V2",
  "enableFileListsIndexing": false,
  "optionalIndexCompressionFormats": []
}
